{
  "extensions": [
    {
      "name": "SoundCloud",
      "description": "Stream music from SoundCloud in Echo Nightly",
      "icon": "https://soundcloud.com/favicon.ico",
      "url": "https://soundcloud.com",
      "script": "https://raw.githubusercontent.com/ecstacy228/echo-soundcloud/main/soundcloud.js",
      "match": ["*://soundcloud.com/*", "*://*.soundcloud.com/*"]
    }
  ]
}
